{
  "name": "item-curator",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.1.1",
    "@material-ui/core": "^4.2.1",
    "@tourlane/tourlane-ui": "^4.0.6",
    "axios": "^0.19.0",
    "cuid": "^2.1.6",
    "cypress": "^3.4.0",
    "i18n-iso-countries": "^4.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-spinners": "^0.5.13",
    "recompose": "^0.30.0",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:staging": "npx env-cmd -f .env.staging react-scripts start",
    "start:production": "npx env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "build:staging": "npx env-cmd -f .env.staging react-scripts build",
    "build:production": "npx env-cmd -f .env.production react-scripts build",
    "deploy": "yarn lint && yarn test:e2e:ci",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@tourlane/tourlane-ui))/\" --env=jsdom",
    "e2e": "npx cypress open",
    "eject": "react-scripts eject",
    "lint": "npx eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "yarn lint --fix",
    "commit": "npx git-cz",
    "release": "npx standard-version",
    "start-server": "yarn build && yarn start",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test start-server http://localhost:3000 cy:run",
    "test:e2e:ci": "CI=1 yarn add cypress; yarn test:e2e"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@sentry/browser": "^5.5.0",
    "commitizen": "^4.0.3",
    "cypress-file-upload": "^3.3.2",
    "cz-conventional-changelog": "^3.0.2",
    "env-cmd": "^9.0.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "standard-version": "^6.0.1",
    "start-server-and-test": "^1.9.1",
    "yargs": "^13.3.0"
  },
  "homepage": "/"
}
