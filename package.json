{
  "name": "item-curator",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.12.1",
    "@material-ui/core": "^4.2.1",
    "@tourlane/iconography": "^2.28.1",
    "@tourlane/rooster": "^4.2.1",
    "@tourlane/tourlane-ui": "^7.23.1",
    "awesome-phonenumber": "^2.33.0",
    "cuid": "^2.1.6",
    "cypress": "^3.7.0",
    "draftjs-to-html": "^0.8.4",
    "lodash": "^4.17.15",
    "query-string": "^6.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.1",
    "react-infinite-scroller": "^1.2.4",
    "react-lazyload": "^2.6.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-spinners": "^0.5.13",
    "react-spring": "^8.0.27",
    "recompose": "^0.30.0",
    "shutterstock-api": "^1.0.15",
    "sinon": "^7.3.2",
    "styled-components": "^4.3.2",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "start:testing": "env-cmd -f .env.testing react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:testing": "env-cmd -f .env.testing react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@tourlane/tourlane-ui|react-spring))/\" --env=jsdom --runInBand",
    "test:silent": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@tourlane/tourlane-ui|react-spring))/\" --silent --env=jsdom --runInBand",
    "test:ci": "CI=true npm run test:silent",
    "e2e": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "npm run lint --fix",
    "commit": "git-cz",
    "release": "standard-version",
    "start-server": "npm run build && npm start",
    "start-server:staging": "npm run build:staging && npm start",
    "start-server:testing": "npm run build:testing && npm run start:testing",
    "serve": "serve -s build -l 3000",
    "cy:run": "cypress run",
    "test:e2e": "CI=1 npm install cypress; start-server-and-test start-server:testing http://localhost:3000 cy:run",
    "serve:lhci": "NODE_ENV=production npm run serve",
    "lighthouse": "lhci autorun"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@sentry/browser": "^5.5.0",
    "@types/lodash": "^4.14.167",
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "commitizen": "^4.0.3",
    "cypress-file-upload": "^3.3.4",
    "cz-conventional-changelog": "^3.0.2",
    "draft-js": "^0.11.0",
    "env-cmd": "^9.0.3",
    "enzyme": "^3.10.0",
    "husky": "^3.0.1",
    "jest-styled-components": "^6.3.3",
    "lighthouse": "^5.5.0",
    "npx": "^10.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^1.11.1",
    "react-draft-wysiwyg": "^1.13.2",
    "react-html-parser": "^2.0.2",
    "react-paginating": "^1.3.0-hook.2",
    "react-responsive-carousel": "^3.1.50",
    "react-scroll": "^1.7.12",
    "react-sortablejs": "^1.5.1",
    "react-tabs": "^3.0.0",
    "react-test-renderer": "^16.11.0",
    "react-truncate-markup": "^4.0.0",
    "serve": "^11.2.0",
    "sortablejs": "^1.10.0-rc3",
    "standard-version": "^8.0.1",
    "start-server-and-test": "^1.9.1"
  },
  "homepage": "/"
}
