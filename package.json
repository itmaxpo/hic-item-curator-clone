{
  "name": "item-curator",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@tourlane/iconography": "^2.121.2",
    "@tourlane/rooster": "^4.8.2",
    "@tourlane/tourlane-ui": "^7.48.1",
    "awesome-phonenumber": "^2.73.0",
    "cuid": "^2.1.6",
    "draftjs-to-html": "^0.9.1",
    "html-react-parser": "^3.0.4",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.7",
    "react-spring": "^8.0.27",
    "react-truncate-markup": "^5.1.2",
    "shutterstock-api": "^1.1.30",
    "styled-components": "^5.3.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "start:testing": "env-cmd -f .env.testing react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:testing": "env-cmd -f .env.testing react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@tourlane/tourlane-ui|react-spring))/\" --env=jsdom --runInBand --passWithNoTests",
    "test:silent": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@tourlane/tourlane-ui|react-spring|react-spinners))/\" --silent --env=jsdom --maxWorkers=2 --passWithNoTests",
    "test:ci": "CI=true npm run test:silent",
    "e2e": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "npm run lint --fix",
    "tsc": "tsc --noEmit",
    "serve": "serve -s build -l 3000",
    "cy:run": "cypress run --browser chrome --headless",
    "test:e2e": "CI=1 npm install cypress; start-server-and-test start:testing http://localhost:3000 cy:run"
  },
  "e2e": {
    "baseUrl": "http://localhost:3000/",
    "viewportWidth": 1440,
    "viewportHeight": 747,
    "video": false,
    "experimentalFetchPolyfill": true,
    "retries": 1,
    "pageLoadTimeout": 10000
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^2.0.3",
    "@sentry/browser": "^6.19.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.181",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-lazyload": "^3.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "cypress": "^11.2.0",
    "cypress-file-upload": "^5.0.8",
    "draft-js": "^0.11.0",
    "env-cmd": "^10.1.0",
    "husky": "^7.0.4",
    "jest-styled-components": "^7.0.8",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-draft-wysiwyg": "^1.14.7",
    "react-hook-form": "^7.40.0",
    "react-paginating": "^1.3.0-hook.2",
    "react-responsive-carousel": "^3.2.23",
    "react-scroll": "^1.8.7",
    "react-sortablejs": "^1.5.1",
    "react-tabs": "^4.2.0",
    "react-test-renderer": "^16.11.0",
    "serve": "^13.0.2",
    "sortablejs": "^1.10.0-rc3",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4"
  },
  "homepage": "/"
}
